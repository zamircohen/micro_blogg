<%- include("partials/header.ejs") %>

        <h1>HASHTAGS</h1>

        <ul>
            <% matches.forEach((item) => { %>
            <li> 
                <img src="/<%= item.entryPhoto %>" alt="profile photo" height="50px" width="50px">
                <%= item.entry%> <i><small>- posted by <a href="/users/<%= item.entryUser%>"><b><%= item.entryUser %></b></a> on <%= item.entryDateString %> </small></i>
            </li>
            <% }) %>
        </ul>


